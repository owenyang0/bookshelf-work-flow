{
  "files": {
    "stylesheets": {
      "main": [
        "src/main/webapp/sass/bookshelf.scss"
      ],
      "src": [
        "src/main/webapp/sass/{,**/}**.scss"
      ],
      "dest": "src/main/webapp/dest/css/"
    },
    "javascript": {
      "src": [
        "src/main/webapp/js/*.js"
      ],
      "dest": "src/main/webapp/dest/js/",
      "destfile": "bookshelf.min.js"
    },
    "images": {
      "src": [
        "src/main/webapp/images/{,**/}**.*"
      ],
      "dest": "src/main/webapp/dest/images/"
    }
  }
}
